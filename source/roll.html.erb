---
notitle: true
---
		<section class="flex flex-col py-4" id="roll_<%= roll_number %>">
      <header>
        <a href="/" class="text-2xl -ml-7"  >‚Üê</a>
        <span class="roll_number" >#<%= roll_number %> - <%= scanned_at %></span>
        <h1 class="mt-4 text-xl font-bold ">üì∑ <%= camera %> - <%=film %></h1>
      </header>
      <dl class="my-4 lists">
        <dt class="font-bold">Tags</dt>
        <dd><%= tags.join(", ") %></dd>
        <dt class="font-bold">Places</dt>
        <dd><%= places.join(", ") %></dd>
      </dl>
			<div class="grid grid-cols-3 gap-4 -mx-32">
        <% files.each do |file| %>
					<%= image_tag(thumb_path(File.join(dir, file)), class: "w-full") %>
        <% end %>
			</div>
		</section>
